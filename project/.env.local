VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
VITE_MISTRAL_API_KEY=your_mistral_api_key_here

# =================================
# VIRTUAL AGENTS CONFIGURATION
# =================================

# Agent System Feature Flags
VITE_AGENTS_DISABLED=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_FALLBACK=true

# Agent Proxy Configuration (local development)
VITE_AGENT_PROXY_PATH=http://localhost:3001/api/agent-proxy

# Local LLM Fallback Configuration
VITE_OLLAMA_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama3.2:3b
VITE_VLLM_URL=http://localhost:8000/v1
VITE_VLLM_MODEL=meta-llama/Llama-2-7b-chat-hf

# Default Model Configuration
VITE_DEFAULT_OLLAMA_MODEL=llama3.2:3b
VITE_DEFAULT_VLLM_MODEL=meta-llama/Llama-2-7b-chat-hf
VITE_DEFAULT_LOCALAI_MODEL=ggml-gpt4all-j

# =================================
# FEATURE FLAGS
# =================================

# Core Features
VITE_ENABLE_AI_ADVISORS=true
VITE_ENABLE_BLOG=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AUTO_SAVE=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_BACKUPS=true

# =================================
# DEVELOPMENT SETTINGS
# =================================

# Enable debug logging
VITE_DEBUG=true
VITE_LOG_LEVEL=debug
VITE_DEV_MODE=true

# CORS settings for local development
VITE_CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:4173

# =================================
# SECURITY CONFIGURATION
# =================================

# Security headers (replicated from netlify.toml)
VITE_ENABLE_SECURITY_HEADERS=true
VITE_CSP_POLICY=default-src 'self' 'unsafe-inline' 'unsafe-eval'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' http://localhost:* ws://localhost:*

# =================================
# API CONFIGURATION
# =================================

# API Configuration
VITE_API_URL=http://localhost:5173
VITE_API_TIMEOUT=30000

# Rate Limiting
VITE_FORM_RATE_LIMIT=5
VITE_FORM_RATE_WINDOW=60000

# File Upload Settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,ppt,pptx

# =================================
# PWA SETTINGS
# =================================

VITE_PWA_ENABLED=true
VITE_PWA_REGISTER_TYPE=prompt
