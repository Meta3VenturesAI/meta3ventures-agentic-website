{
  "name": "meta3ventures",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:sitemap && vite build",
    "build:analyze": "npm run generate:sitemap && ANALYZE=true vite build",
    "build:production": "NODE_ENV=production npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 600",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "agent:eval": "node scripts/agent-eval.mjs",
    "agent:eval:ci": "AGENT_SEED=7 AGENT_BACKEND=ollama node scripts/agent-eval.mjs",
    "verify:phaseD": "node scripts/verify-phase-d.mjs",
    "verify:phaseD:strict": "npm run build && npm run verify:phaseD",
    "test": "vitest run --config vitest.config.ts --coverage",
    "test:watch": "vitest --config vitest.config.ts",
    "deploy:netlify": "npm run build:production && netlify deploy --prod",
    "deploy:vercel": "npm run build:production && vercel --prod",
    "deploy": "sh scripts/deploy.sh",
    "clean": "rm -rf dist node_modules/.vite coverage",
    "build:clean": "npm run clean && npm run build",
    "preflight": "npm run lint && npm run type-check && npm run test",
    "dev:local": "node local-dev-server.js",
    "test:full-platform": "node test-full-platform.cjs",
    "setup:local": "bash ../setup-local-testing.sh",
    "cleanup:local": "bash ../cleanup-local-testing.sh",
    "smoke:agents": "tsx scripts/agent-smoke.ts",
    "enable:agents": "tsx scripts/enable-agents.ts",
    "test:agents": "tsx scripts/simple-agent-test.ts",
    "enable:agents:simple": "tsx scripts/enable-agents-simple.ts",
    "test:agent-system": "tsx scripts/test-agent-system.ts",
    "test:agents-page": "tsx scripts/test-agents-page.ts",
    "test:e2e": "tsx scripts/run-comprehensive-tests.ts",
    "test:unit": "tsx -e \"import { runCriticalUnitTests } from './src/test/unit-tests'; runCriticalUnitTests();\"",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test"
  },
  "dependencies": {
    "@formspree/react": "^2.5.1",
    "@sentry/react": "^10.8.0",
    "@supabase/supabase-js": "^2.39.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "bcryptjs": "^3.0.2",
    "date-fns": "^3.3.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "tailwindcss": "^3.4.1",
    "uuid": "^13.0.0",
    "vite": "^6.3.6",
    "vite-plugin-pwa": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.5",
    "typescript": "^5.8.4",
    "typescript-eslint": "^8.3.0",
    "undici": "^6.21.3",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
