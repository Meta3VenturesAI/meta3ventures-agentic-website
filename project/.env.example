# Meta3Ventures Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Formspree Configuration (Required)
VITE_FORMSPREE_KEY=your_formspree_key

# Sentry Configuration (Optional - for error tracking)
VITE_SENTRY_DSN=your_sentry_dsn

# Authentication Configuration (Required)
VITE_ADMIN_PASSWORD=your_secure_admin_password
VITE_AUTH_SECRET=your_jwt_secret_key

# LLM Provider Configuration (Optional - for AI agents)
VITE_OLLAMA_BASE_URL=http://localhost:11434
VITE_VLLM_BASE_URL=http://localhost:8000/v1
VITE_GROQ_API_KEY=your_groq_api_key

# Security Configuration (Required)
VITE_ENABLE_SECURITY_HEADERS=true

# Development Configuration
VITE_DEV_MODE=false
VITE_DEBUG_AGENTS=false

# External Services (Optional)
VITE_DROPBOX_ACCESS_TOKEN=your_dropbox_token
VITE_GOOGLE_ANALYTICS_ID=your_ga_id

# Feature Flags
VITE_ENABLE_AGENTS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
